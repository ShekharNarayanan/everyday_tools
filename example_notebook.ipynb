      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 8,
      "metadata": {},
      "outputs": [],
      "source": [
        "x = np.random.randint(low=10,high=20,size=100)\n",
        "y = np.random.permutation(x)\n",
        "\n",
        "print(x.shape)\n",
        "print(y.shape)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 9,
      "metadata": {},
      "outputs": [],
      "source": [
        "plt.scatter(x,x*y,color='r',marker='o')\n",
        "plt.title('random plot')\n",
        "plt.show()"
      ]
    },
